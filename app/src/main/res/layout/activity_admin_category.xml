<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/grey_color"
    tools:context=".AdminCategoryActivity">


    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="158dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="69dp"
        android:orientation="horizontal">


        <ImageView
            android:id="@+id/Shoes"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:scaleType="centerCrop"
            android:src="@drawable/shoess"></ImageView>

        <ImageView
            android:id="@+id/TShirts"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:scaleType="centerCrop"
            android:src="@drawable/tshirts">

        </ImageView>

        <ImageView
            android:id="@+id/Laptop"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:scaleType="centerCrop"
            android:src="@drawable/laptops"></ImageView>


        <ImageView
            android:id="@+id/Watches"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:scaleType="centerCrop"
            android:src="@drawable/watches"></ImageView>

    </LinearLayout>


    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="401dp"
        android:layout_height="158dp"
        android:layout_below="@+id/linearLayout"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="17dp"
        android:orientation="horizontal">


        <ImageView
            android:id="@+id/HeadPhones"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:scaleType="centerCrop"
            android:src="@drawable/headphoness"></ImageView>

        <ImageView
            android:id="@+id/Shirts"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:scaleType="centerCrop"
            android:src="@drawable/sports"></ImageView>

        <ImageView
            android:id="@+id/Dress"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:scaleType="centerCrop"
            android:src="@drawable/sweather">

        </ImageView>

        <ImageView
            android:id="@+id/Purse"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:scaleType="centerCrop"
            android:src="@drawable/purses_bags"></ImageView>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="412dp"
        android:layout_height="99dp"
        android:layout_below="@+id/linearLayout1"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"

        android:layout_marginTop="13dp"
        android:orientation="horizontal">


        <ImageView
            android:id="@+id/Phones"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:scaleType="centerCrop"
            android:src="@drawable/mobiles"></ImageView>

        <ImageView
            android:id="@+id/Female_Dress"
            android:layout_width="wrap_content"
            android:layout_height="90dp"
            android:scaleType="centerCrop"
            android:src="@drawable/female_dresses"></ImageView>

        <ImageView
            android:id="@+id/Hats"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:scaleType="centerCrop"
            android:src="@drawable/hats">

        </ImageView>

        <ImageView
            android:id="@+id/Glasses"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:scaleType="centerCrop"
            android:src="@drawable/glasses"></ImageView>

    </LinearLayout>


</RelativeLayout>